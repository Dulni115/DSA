class QueueStackPushFriendly:
    def __init__(self):
        self.queue1 = []
        self.queue2 = []
    
    def push(self, x):
        """Push friendly: O(1) push"""
        self.queue1.append(x)
    
    def pop(self):
        """O(n) pop"""
        if not self.queue1:
            return None
        
        # Move all except last to queue2
        while len(self.queue1) > 1:
            self.queue2.append(self.queue1.pop(0))
        
        # Get the last element (which is the top of stack)
        result = self.queue1.pop(0)
        
        # Swap queues
        self.queue1, self.queue2 = self.queue2, self.queue1
        
        return result
    
    def top(self):
        if not self.queue1:
            return None
        
        while len(self.queue1) > 1:
            self.queue2.append(self.queue1.pop(0))
        
        result = self.queue1[0]
        self.queue2.append(self.queue1.pop(0))
        
        # Swap queues
        self.queue1, self.queue2 = self.queue2, self.queue1
        
        return result
    
    def is_empty(self):
        return len(self.queue1) == 0
    
    def size(self):
        return len(self.queue1)
