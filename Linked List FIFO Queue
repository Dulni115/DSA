class QueueNode:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedListFIFOQueue:
    def __init__(self):
        self.front = None
        self.rear = None
    
    def enqueue(self, data):
        new_node = QueueNode(data)
        
        if not self.rear:
            self.front = self.rear = new_node
        else:
            self.rear.next = new_node
            self.rear = new_node
    
    def dequeue(self):
        if not self.front:
            return None
        
        dequeued = self.front.data
        self.front = self.front.next
        
        if not self.front:
            self.rear = None
        
        return dequeued
    
    def peek(self):
        return self.front.data if self.front else None
    
    def is_empty(self):
        return self.front is None
    
    def display(self):
        current = self.front
        while current:
            print(current.data, end=" -> ")
            current = current.next
        print("None")

# Usage
queue = LinkedListFIFOQueue()
queue.enqueue(10)
queue.enqueue(20)
queue.enqueue(30)
print("Dequeued:", queue.dequeue())
print("Front:", queue.peek())
queue.display()
