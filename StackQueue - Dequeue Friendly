class StackQueueDequeueFriendly:
    def __init__(self):
        self.stack1 = []  # For enqueue operations
        self.stack2 = []  # For dequeue operations
    
    def enqueue(self, x):
        """O(n) enqueue"""
        # Move all from stack2 to stack1
        while self.stack2:
            self.stack1.append(self.stack2.pop())
        
        # Add new element
        self.stack1.append(x)
        
        # Move all back to stack2
        while self.stack1:
            self.stack2.append(self.stack1.pop())
    
    def dequeue(self):
        """Dequeue friendly: O(1) dequeue"""
        if self.is_empty():
            return None
        return self.stack2.pop()
    
    def front(self):
        if self.is_empty():
            return None
        return self.stack2[-1]
    
    def is_empty(self):
        return not self.stack2
    
    def size(self):
        return len(self.stack2)
